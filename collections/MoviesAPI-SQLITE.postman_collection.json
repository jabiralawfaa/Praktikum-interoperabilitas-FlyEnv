						"exec": [
							"// Cek apakah response sukses (status code 200)",
							"pm.test(\"Status code is 200\", function () {",
							"    pm.response.to.have.status(200);",
							"});",
							"",
							"// Jika sukses, ambil token dan simpan ke GLOBAL variable",
							"pm.test(\"Set auth token globally\", function () {",
							"    const jsonData = pm.response.json();",
							"    ",
							"    if (jsonData && jsonData.token) {",
							"        // Simpan token ke variabel GLOBAL bernama 'authToken'",
							"        pm.globals.set(\"authToken\", jsonData.token);",
							"        console.log(\"Token berhasil disimpan secara GLOBAL!\");",
							"    } else {",
							"        console.log(\"Token tidak ditemukan di response.\");",
